{
  "name": "contexture-client",
  "version": "2.47.1",
  "description": "The Contexture (aka ContextTree) Client",
  "main": "lib/contexture-client.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint --ignore-path .gitignore './**/*.js'",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --json --outputFile test-results.json",
    "fmt": "prettier --ignore-path .gitignore --write './**/*.js'",
    "danger": "duti",
    "duti:fix": "npm run lint:fix; npm run fmt",
    "prepublish": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture-client.git"
  },
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-client/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-client#readme",
  "dependencies": {
    "futil": "^1.69.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "danger": "^10.5.4",
    "duti": "^0.15.2",
    "eslint": "^4.12.1",
    "eslint-config-smartprocure": "^1.1.0",
    "jest": "^29.0.1",
    "jest-esbuild": "^0.2.9",
    "mobx": "^4.3.1",
    "prettier": "^1.8.2",
    "webpack": "^3.8.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-runtime"
    ]
  }
}
