{
  "name": "contexture-export",
  "version": "0.2.1",
  "description": "Contexture Exports",
  "main": "lib/contexture-export.js",
  "scripts": {
    "build": "webpack",
    "danger": "duti",
    "lint": "eslint src/*.js test/*.js",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "lint-fix": "eslint src/*.js test/*.js --fix",
    "fmt": "prettier --write *.js; prettier --write '{src,test}/**/*.js'",
    "duti:fix": "npm run fmt && npm run lint-fix && npm run fmt",
    "test": "jest --",
    "test:watch": "jest --watch --",
    "test:ci": "jest --coverage --json --outputFile=test-results.json",
    "coverage": "jest --coverage --",
    "prepublish": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sadasant@github.com/smartprocure/contexture-export.git"
  },
  "author": "SmartProcure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-export/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-export#readme",
  "peerDependencies": {
    "futil": "^1.55.0",
    "lodash": "^4.17.11"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "smart-truncate": "^1.0.1"
  },
  "devDependencies": {
    "futil": "^1.55.0",
    "lodash": "^4.17.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "duti": "^0.14.0",
    "eslint": "^5.6.1",
    "eslint-config-smartprocure": "^1.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "prettier": "^1.14.3",
    "webpack": "^3.8.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-runtime"
    ]
  }
}
