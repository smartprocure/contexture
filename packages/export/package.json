{
  "name": "contexture-export",
  "version": "1.0.10",
  "description": "Contexture Exports",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./*": {
      "import": "./src/*"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./*": {
        "import": "./dist/esm/*",
        "require": "./dist/cjs/*"
      }
    }
  },
  "scripts": {
    "prepack": "node scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sadasant@github.com/smartprocure/contexture.git"
  },
  "author": "SmartProcure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture/tree/main/packages/export",
  "dependencies": {
    "futil": "^1.71.8",
    "lodash": "^4.17.21",
    "minimal-csv-formatter": "^1.0.15"
  },
  "devDependencies": {
    "@flex-development/toggle-pkg-type": "^1.0.1",
    "esbuild": "^0.17.0",
    "glob": "^8.0.3"
  },
  "packageManager": "yarn@3.3.1"
}
