{
  "name": "contexture-export",
  "version": "1.0.1",
  "description": "Contexture Exports",
  "main": "lib/contexture-export.js",
  "scripts": {
    "build": "webpack",
    "duti": "duti",
    "danger": "danger ci",
    "lint": "eslint src/*.js test/*.js",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "prettier --write *.js; prettier --write '{src,test}/**/*.js'",
    "duti:fix": "npm run lint:fix; npm run fmt",
    "test": "jest --",
    "test:watch": "jest --watch --",
    "test:ci": "jest --coverage --json --outputFile=test-results.json",
    "prepublish": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sadasant@github.com/smartprocure/contexture-export.git"
  },
  "author": "SmartProcure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-export/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-export#readme",
  "peerDependencies": {
    "futil": "^1.55.0",
    "lodash": "^4.17.11"
  },
  "dependencies": {
    "minimal-csv-formatter": "^1.0.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "danger": "^10.5.4",
    "duti": "^0.15.2",
    "eslint": "^5.6.1",
    "eslint-config-smartprocure": "^1.2.0",
    "futil": "^1.55.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.3",
    "webpack": "^3.8.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
