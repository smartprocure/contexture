{
  "name": "contexture-react",
  "version": "1.54.4",
  "description": "React components for building contexture interfaces",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run storybook",
    "pre-build": "rm -rf ./dist",
    "build": "babel ./src --out-dir ./dist --source-maps",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint src stories .storybook .circleci __tests__",
    "lint-fix": "npm run lint -- --fix",
    "lint-ci": "npm run lint -- -o lint-results.json -f json",
    "test": "jest --coverage --passWithNoTests",
    "test-ci": "npm t -- --json --outputFile=test-results.json",
    "duti": "duti",
    "fmt": "prettier --write '{src,stories,.storybook}/**/*'",
    "duti:fix": "npm run lint-fix && npm run fmt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture-react.git"
  },
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-react/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-react#readme",
  "peerDependencies": {
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.0",
    "mobx-utils": "^5.0.0",
    "react": "^16.8.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [ci skip]"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(md)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-info": "^3.4.12",
    "@storybook/addon-storysource": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "contexture": "^0.2.1",
    "contexture-client": "^2.25.0",
    "contexture-elasticsearch": "^0.10.2",
    "danger": "^6.1.13",
    "duti": "^0.15.0",
    "elasticsearch-browser": "^14.2.2",
    "eslint": "^4.12.1",
    "eslint-config-smartprocure": "^1.1.0",
    "jest": "^24.7.1",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.0",
    "mobx-utils": "^5.0.0",
    "prettier": "^1.8.2",
    "raw-loader": "^2.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-select": "^2.0.0",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-jsx": "^7.1.0"
  },
  "dependencies": {
    "futil-js": "^1.58.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.7.2",
    "react-date-picker": "^7.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-outside-click-handler": "^1.2.3",
    "recompose": "^0.30.0"
  }
}
