{
  "name": "contexture-react",
  "version": "0.0.2",
  "description": "React components for building contexture interfaces",
  "main": "index.js",
  "scripts": {
    "pre-build": "rm -rf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "test": "jest --coverage",
    "test-u": "jest --coverage --u",
    "duti": "duti",
    "fmt": "prettier --write '{src,stories,__tests__,.storybook}/**/*'",
    "duti:fix": "npm run fmt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture-react.git"
  },
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-react/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-react#readme",
  "peerDependencies": {
    "react": "^15.x"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "duti": "^0.9.3",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  }
}
