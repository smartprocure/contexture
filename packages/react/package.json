{
  "name": "contexture-react",
  "version": "0.0.4",
  "description": "React components for building contexture interfaces",
  "main": "index.js",
  "scripts": {
    "pre-build": "rm -rf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint src stories .storybook .circleci __tests__",
    "lint-fix": "npm run lint -- --fix",
    "lint-ci": "npm run lint -- -o lint-results.json -f json",
    "test": "jest --coverage",
    "test-u": "jest --coverage --u",
    "test-ci": "npm t -- --json --outputFile=test-results.json",
    "duti": "duti",
    "fmt": "prettier --write '{src,stories,__tests__,.storybook}/**/*'",
    "duti:fix": "npm run lint-fix && npm run fmt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture-react.git"
  },
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-react/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-react#readme",
  "peerDependencies": {
    "react": "^15.x"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [ci skip]"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(md)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "duti": "^0.9.3",
    "eslint": "^4.12.1",
    "eslint-config-smartprocure": "^1.1.0",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  }
}
