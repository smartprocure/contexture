{
  "name": "contexture-react",
  "version": "1.39.3",
  "description": "React components for building contexture interfaces",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run storybook",
    "pre-build": "rm -rf ./dist",
    "build": "babel ./src --out-dir ./dist --source-maps",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint src stories .storybook .circleci __tests__",
    "lint-fix": "npm run lint -- --fix",
    "lint-ci": "npm run lint -- -o lint-results.json -f json",
    "test": "jest --coverage",
    "test-u": "jest --coverage --u",
    "test-ci": "npm t -- --json --outputFile=test-results.json",
    "duti": "duti",
    "fmt": "prettier --write '{src,stories,__tests__/Snapshot.js,.storybook}/**/*'",
    "duti:fix": "npm run lint-fix && npm run fmt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture-react.git"
  },
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture-react/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture-react#readme",
  "peerDependencies": {
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.0",
    "mobx-utils": "^5.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "storybook-deployer": {
    "commitMessage": "Deploy Storybook [ci skip]"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(md)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addon-options": "^3.4.2",
    "@storybook/addon-storyshots": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "contexture": "^0.2.1",
    "contexture-client": "^2.18.0",
    "contexture-elasticsearch": "^0.10.2",
    "duti": "latest",
    "elasticsearch-browser": "^14.2.2",
    "eslint": "^4.12.1",
    "eslint-config-smartprocure": "^1.1.0",
    "jest": "^22.4.3",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.0",
    "mobx-utils": "^5.0.0",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-select": "^2.0.0",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-jsx": "^5.3.0"
  },
  "dependencies": {
    "futil-js": "^1.55.0",
    "lodash": "^4.17.4",
    "react-date-picker": "^7.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "recompose": "^0.30.0"
  }
}
