{
  "extends": ["plugin:react/recommended"],
  "ignorePatterns": [
    ".storybook",
    "*.stories.js",
    "src/stories/EmojiIcon.js",
    "src/stories/basicTagsQuerySearch.js"
  ],
  "parserOptions": {
    "ecmaFeatures": { "jsx": true }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/display-name": "off",
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
