{
  "extends": [
    "plugin:react/recommended",
    "plugin:storybook/recommended"
  ],
  "ignorePatterns": [
    "!.storybook"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/display-name": "off",
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
