{
  "extends": ["plugin:react/recommended"],
  "ignorePatterns": ["!.storybook"],
  "parserOptions": {
    "ecmaFeatures": { "jsx": true }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/display-name": "off",
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
