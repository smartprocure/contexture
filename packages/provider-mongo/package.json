{
  "name": "contexture-mongo",
  "version": "0.26.8",
  "description": "Mongo Provider for Contexture",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./*": {
      "import": "./src/*"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./*": {
        "import": "./dist/esm/*",
        "require": "./dist/cjs/*"
      }
    }
  },
  "scripts": {
    "prepack": "node scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartprocure/contexture.git"
  },
  "keywords": [
    "contexture",
    "mongo"
  ],
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture/tree/main/packages/provider-mongo",
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@elastic/datemath": "^2.3.0",
    "bluebird": "^3.5.0",
    "debug": "^4.3.1",
    "futil": "^1.71.8",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.28"
  },
  "devDependencies": {
    "@flex-development/toggle-pkg-type": "^1.0.1",
    "contexture": "^0.0.4",
    "esbuild": "^0.16.17",
    "glob": "^8.0.3",
    "mingo": "^4.1.2",
    "mongodb": "^3.5.6",
    "mongodb-memory-server": "^6.5.2"
  },
  "peerDependencies": {
    "mongodb": "^3.5.6"
  }
}
